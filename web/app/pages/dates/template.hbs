<document>
   <stackTemplate>
      <banner>
         <title>Podle data</title>
      </banner>
      <collectionList>
        <grid>
          <section>
            {{#each results }}
            <lockup data-href-page="dates-date" data-href-page-options="{{toJSON this }}">
              <img src="{{{asset_url poster_path}}}" style="width:200; height:200;"/>
              <title>{{ short_title }}</title>
            </lockup>
            {{/each}}
          </section>
        </grid>
      </collectionList>
   </stackTemplate>
</document>
