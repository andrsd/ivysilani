  "description": "Neoficiální klient iVysílání pro Apple TV s použitím atvjs",
  "main": "index.js",
  "scripts": {
    "install-deps": "npx yarn@$npm_package_engines_yarn install --frozen-lockfile --check-files",
    "build": "webpack --progress",
    "serve": "webpack-dev-server",
    "dist": "NODE_ENV=production webpack --progress",
    "test": "./node_modules/.bin/mocha test --timeout 0 --exit --require @babel/register",
    "coverage": "npx nyc --reporter=html npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrsd/ivysilani.git"
  },
  "keywords": [
    "appletv",
    "tvml",
    "app",
    "tvjs",
    "atvjs",
    "appletv",
    "tvmlkit",
    "ivysilani"
  ],
  "author": "David Andrš",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrsd/ivysilani/issues"
  },
  "homepage": "https://github.com/andrsd/ivysilani#readme",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/register": "^7.15.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "handlebars-loader": "^1.7.1",
    "mocha": "^9.2.2",
    "nyc": "^15.0.1",
    "raw-loader": "^4.0.0",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.3"
  },
  "dependencies": {
    "atvjs": "^0.2.9",
    "fast-xml-parser": "^3.19.0",
    "handlebars": "^4.7.7"
  }
}
